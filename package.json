{
  "name": "learner-model-gql-template",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "build": "next build",
    "dev": "concurrently -r next pnpm:gql:watch pnpm:open:browser",
    "gql": "graphql-codegen --config codegen.yaml",
    "gql:watch": "graphql-codegen --config codegen.yaml --watch",
    "open:browser": "wait-on -s 1 http://localhost:3000 && open-cli http://localhost:3000",
    "pretty": "prettier --write \"**/*.{ts,tsx,json}\"",
    "start": "next start"
  },
  "dependencies": {
    "@auth0/auth0-react": "^1.8.0",
    "@chakra-ui/react": "^1.7.0",
    "@chakra-ui/system": "^1.8.0",
    "@emotion/react": "^11.5.0",
    "@emotion/styled": "^11.3.0",
    "@graphql-codegen/cli": "^2.2.2",
    "@graphql-codegen/core": "^2.3.0",
    "@graphql-typed-document-node/core": "^3.1.0",
    "framer-motion": "^5.3.0",
    "graphql": "^16.0.1",
    "next": "^12.0.3",
    "immutability-helper": "^3.1.1",
    "katex": "^0.13.13",
    "react-dnd": "^14.0.3",
    "@matejmazur/react-katex": "^3.1.3",
    "react-dnd-html5-backend": "^14.0.1",
    "react-dnd-touch-backend": "^14.1.0",
    "react-notification-badge": "^1.5.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-icons": "^4.3.1",
    "react-query": "^3.32.1",
    "rq-gql": "^0.3.1",
    "serialize-error": "^8.1.0"
  },
  "devDependencies": {
    "@graphql-codegen/schema-ast": "^2.4.0",
    "@types/concurrently": "^6.3.0",
    "@types/react": "^17.0.34",
    "@types/react-dom": "^17.0.11",
    "concurrently": "^6.3.0",
    "open-cli": "^7.0.1",
    "prettier": "^2.4.1",
    "typescript": "^4.4.4",
    "wait-on": "^6.0.0"
  }
}
